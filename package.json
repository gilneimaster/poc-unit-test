{
  "name": "minha-frota-mfe-angular-poc-unit-test",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "ng build",
    "postinstall": "ngcc",
    "watch": "ng build --watch --configuration development",
    "test": "web-test-runner --node-resolve --coverage",
    "sonar": "sonar-scanner -Dsonar.projectVersion=$(jq -r .version package.json)",
    "sonar:dev": "sonar-scanner -Dsonar.projectVersion=$(jq -r .version package.json) -Dsonar.login=\"admin\" -Dsonar.password=\"dev\" ",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^12.5.3",
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "rxjs": "~6.6.0",
    "subsink": "^1.0.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@web/dev-server-esbuild": "^0.4.1",
    "@web/dev-server-import-maps": "^0.1.1",
    "@web/test-runner": "^0.17.0",
    "@web/test-runner-playwright": "^0.10.1",
    "jasmine-core": "~3.8.0",
    "typescript": "~4.2.3"
  }
}
